    "web": "node src/web/server.js",
    "web:dev": "WEB_BIND=0.0.0.0 WEB_PORT=8090 node src/web/server.js",
    "web:smoke": "node src/web/smoke.js",
    "test": "node --test"
  },
  "dependencies": {
    "@langchain/openai": "^1.2.3",
    "bcryptjs": "^2.4.3",
    "dotenv": "^17.2.3",
