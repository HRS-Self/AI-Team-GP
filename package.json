  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "intake": "node src/intake/server.js",
    "orchestrate": "node src/cli.js",
    "orchestrate:demo": "node src/cli.js --text \"MVP demo intake: add a hello endpoint to Backend service and update portal copy\"",
